{
  "name": "@n8n-clone/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "nx run-many --target=build --all",
    "start": "nx run-many --target=serve --all --maxParallel=12",
    "start:dev": "nx run-many --target=serve --all --configuration=development --maxParallel=12",
    "start:api-gateway": "nx serve api-gateway",
    "start:workflow-orchestrator": "nx serve workflow-orchestrator",
    "start:node-runtime-engine": "nx serve node-runtime-engine",
    "start:trigger-manager": "nx serve trigger-manager",
    "start:credentials-vault": "nx serve credentials-vault",
    "start:execution-history": "nx serve execution-history",
    "start:template-manager": "nx serve template-manager",
    "start:user-management": "nx serve user-management",
    "start:notification-hub": "nx serve notification-hub",
    "start:variable-manager": "nx serve variable-manager",
    "start:queue-processor": "nx serve queue-processor",
    "start:monitoring-service": "nx serve monitoring-service",
    "test": "nx run-many --target=test --all",
    "test:watch": "nx run-many --target=test --all --watch",
    "test:cov": "nx run-many --target=test --all --coverage",
    "test:integration": "nx run-many --target=test --all --testPathPattern=integration",
    "test:e2e": "nx run-many --target=e2e --all",
    "lint": "nx run-many --target=lint --all",
    "lint:fix": "nx run-many --target=lint --all --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\"",
    "docker:dev:up": "cd docker && docker-compose -f docker-compose.dev.yml --env-file .env.dev up -d",
    "docker:dev:down": "cd docker && docker-compose -f docker-compose.dev.yml down",
    "docker:dev:logs": "cd docker && docker-compose -f docker-compose.dev.yml logs -f",
    "docker:dev:infrastructure": "cd docker && docker-compose -f docker-compose.dev.yml --env-file .env.dev up -d postgres-dev redis-dev adminer mailhog",
    "docker:prod:up": "cd docker && docker-compose -f docker-compose.yml up --build -d",
    "docker:prod:down": "cd docker && docker-compose -f docker-compose.yml down",
    "docker:prod:logs": "cd docker && docker-compose -f docker-compose.yml logs -f"
  },
  "private": true,
